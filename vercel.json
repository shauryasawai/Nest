{
  "version": 2,
  "builds": [
    {
      "src": "project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "python": { "useRuntime": true }
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "project/wsgi.py"
    }
  ]
}